{
	"name": "@greenlight/webapp",
	"version": "0.0.1",
	"scripts": {
		"clean": "essex clean dist/ out/ .next node_modules/.cache/",
		"start:webapp": "yarn start",
		"start:webapp:prod": "NODE_ENV='production' yarn start",
		"start": "ts-node --project tsconfig.server.json server.ts",
		"next_build": "next build",
		"server_build": "tsc -b tsconfig.server.json",
		"write_deploy_package": "ts-node --project tsconfig.server.json writeDeployPackage.ts",
		"copy_assets": "shx cp -r config/ .next/ dist/",
		"build": "run-s next_build server_build copy_assets write_deploy_package",
		"prearchive": "cd dist && npm install",
		"archive": "essex zip --baseDir dist deploy.zip '**/*'",
		"plop": "plop",
		"plop:webapp": "plop"
	},
	"dependencies": {
		"@fluentui/react": "^8.14.5",
		"@reduxjs/toolkit": "^1.5.1",
		"axios": "^0.21.1",
		"bootstrap": "^5.0.0-beta3",
		"classnames": "^2.3.1",
		"config": "^3.3.6",
		"dotenv": "^8.6.0",
		"express": "^4.17.1",
		"formik": "^2.2.6",
		"http-proxy-middleware": "^1.3.1",
		"lodash": "^4.17.21",
		"next": "^10.2.0",
		"next-auth": "^3.19.8",
		"plop": "^2.7.4",
		"postcss-flexbugs-fixes": "^5.0.2",
		"postcss-hover-media-feature": "^1.0.1",
		"postcss-preset-env": "^6.7.0",
		"react": "^17.0.2",
		"react-bootstrap": "^1.5.2",
		"react-dom": "^17.0.2",
		"react-dom-confetti": "^0.2.0",
		"react-redux": "^7.2.4",
		"redux": "^4.1.0",
		"redux-persist": "^6.0.0",
		"sass": "^1.32.12",
		"yup": "^0.32.9"
	},
	"license": "MIT",
	"devDependencies": {
		"@essex/scripts": "^16.0.2",
		"@tsconfig/node14": "^1.0.0",
		"@types/config": "^0.0.38",
		"@types/express": "^4.17.11",
		"@types/node": "^14.14.44",
		"@types/react": "^17.0.5",
		"@types/react-dom": "^17.0.4",
		"@types/yup": "^0.29.11",
		"autoprefixer": "^10.2.5",
		"babel-plugin-transform-define": "^1.3.2",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.2.13",
		"shx": "^0.3.3",
		"ts-node": "^9.1.1",
		"typescript": "^4.2.4"
	}
}
