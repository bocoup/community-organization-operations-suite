{
	"name": "@greenlight/webapp",
	"version": "0.0.1",
	"scripts": {
		"start_webapp_server": "ts-node --project tsconfig.server.json server.ts",
		"start:webapp": "yarn start",
		"start": "yarn start_webapp_server",
		"build": "next build",
		"build:webapp": "yarn build",
		"export": "next build && next export",
		"export-and-serve": "next build && next export && serve ./out",
		"lint": "eslint ./",
		"lint:fix": "eslint ./ --fix-dry-run",
		"deploy": "rm -rf node_modules/.cache && next build && next export && touch out/.nojekyll && git add out/ && git commit -m \"Deploy Next.js to gh-pages\" && git subtree push --prefix out origin gh-pages",
		"plop": "plop",
		"plop:webapp": "plop"
	},
