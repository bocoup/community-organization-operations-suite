{
  "name": "@cbosuite/webapp",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "essex clean dist/ out/ .next node_modules/.cache/",
    "start": "yarn assemble:localizations && next dev",
    "start:webapp": "yarn start",
    "build": "run-s clean assemble:localizations next_build next_export",
    "build:webapp:integ": "NODE_CONFIG_ENV=integration yarn build",
    "build:webapp:prod": "NODE_CONFIG_ENV=production yarn build",
    "build:webapp:tenant_integ": "NODE_CONFIG_ENV=tenant-integration yarn build",
    "build:webapp:tenant_prod": "NODE_CONFIG_ENV=tenant-production yarn build",
    "deploy:webapp": "essex deploy --type azure-blob-storage --storageAccount $WEBAPP_STORAGE_ACCOUNT --storageAccountKey $WEBAPP_STORAGE_KEY --verbose --paths out/",
    "next_build": "next build",
    "next_export": "next export",
    "plop": "plop",
    "plop:webapp": "plop",
    "assemble:localizations": "node scripts/generateLocalizationFiles",
    "lint:localizations": "node scripts/lintLocalizationFiles",
    "test": "yarn assemble:localizations && jest && yarn lint:localizations"
  },
  "dependencies": {
    "@apollo/client": "^3.3.20",
    "@cbosuite/schema": "workspace:packages/schema",
    "@fluentui/react": "^8.20.2",
    "@fluentui/react-hooks": "^8.2.2",
    "bootstrap": "^5.0.1",
    "classnames": "^2.3.1",
    "config": "^3.3.6",
    "core-js": "^3.15.2",
    "firebase": "^8.9.1",
    "formik": "^2.2.9",
    "graphql": "^15.5.1",
    "json2csv": "^5.0.6",
    "lodash": "^4.17.21",
    "next": "^10.2.3",
    "next-auth": "^3.27.0",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.1",
    "react-dom": "^17.0.2",
    "react-dom-confetti": "^0.2.0",
    "react-intl": "^5.20.10",
    "react-is": "^17.0.2",
    "react-paginated-list": "^1.1.5",
    "react-select": "^4.3.1",
    "react-toast-notifications": "^2.4.4",
    "react-transition-group": "^4.4.2",
    "recoil": "^0.3.1",
    "recoil-persist": "^2.9.0",
    "styled-components": "^5.3.0",
    "subscriptions-transport-ws": "^0.9.19",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@essex/scripts": "^18.2.0",
    "@tsconfig/node14": "^1.0.1",
    "@types/config": "^0.0.38",
    "@types/express": "^4.17.11",
    "@types/jest": "^27.0.1",
    "@types/json2csv": "^5",
    "@types/node": "^14.17.3",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "@types/react-is": "^17",
    "@types/react-select": "^4",
    "@types/react-transition-group": "^4",
    "@types/styled-components": "^5",
    "@types/yup": "^0.29.11",
    "autoprefixer": "^10.2.6",
    "babel-plugin-transform-define": "^1.3.2",
    "jest": "^27.0.6",
    "npm-run-all": "^4.1.5",
    "plop": "^2.7.4",
    "postcss": "^8.3.5",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-hover-media-feature": "^1.0.1",
    "postcss-preset-env": "^6.7.0",
    "sass": "1.32.13",
    "typescript": "^4.3.5"
  }
}
